    <script>

        $('#select-season-2').on('change', function (event) {
            event.preventDefault();
            let option = $(this).val();
            option = option * 1 + 1
            $('.all-episodes').addClass('hide');
            $(`#season-${option}`).removeClass('hide');
        });


        $('.language-select').on('click', function () {
            const now_id = $(this).attr('data-id')
            const ul_el = $(`#ul-${now_id}`)
            if (ul_el.hasClass('hide')) {
                ul_el.removeClass('hide')
            } else {
                ul_el.addClass('hide')
            }
        })

        $('.clili').on('click', function () {
            const iframe = $('#play-iframe')
            iframe.attr('src', null)
            $('.clili').removeClass('actives')
            $(this).addClass('actives')
            const link = $(this).data('video')
            iframe.attr('src', link)
        })
    </script>

